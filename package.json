{
    "name": "kits-ffgz",
    "displayName": "kits-ffgz",
    "publisher": "feifeigongzhu",
    "description": "this is a kit",
    "repository": {
        "type": "git",
        "url": "https://gitee.com/zwl257/reminder-ffgz.git"
    },
    "version": "0.0.14",
    "icon": "images/icon.jfif",
    "engines": {
        "vscode": "^1.84.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "reminder.setUserName",
                "title": "setUserName"
            },
            {
                "command": "reminder.setReminderTime",
                "title": "setReminderTime"
            },
            {
                "command": "reminder.addConsole",
                "title": "addConsole"
            },
            {
                "command": "reminder.removeConsole",
                "title": "removeConsole"
            },
            {
                "command": "reminder.clearEmptyLines",
                "title": "clearEmptyLines"
            }
        ],
        "configuration": {
            "title": "reminder",
            "properties": {
                "reminder.suffix": {
                    "type": "string",
                    "default": "·",
                    "description": "console前缀"
                },
                "reminder.fixStyle": {
                    "type": "string",
                    "default": "color:#fff;background:#000",
                    "description": "前缀样式"
                },
                "reminder.userName": {
                    "type": "string",
                    "default": "ffgz",
                    "description": "用户名"
                },
                "reminder.reminderTime": {
                    "type": "",
                    "default": "2021-10-01",
                    "description": "提醒间隔时间"
                },
                "reminder.showStatusBar": {
                    "type": "boolean",
                    "default": false,
                    "description": "是否展示运行时间"
                }
            }
        },
        "menus": {
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "reminder.addConsole",
                    "group": "navigation@1"
                }
            ]
        },
        "keybindings": [
            {
                "command": "reminder.addConsole",
                "key": "alt+C",
                "mac": "alt+C",
                "when": "editorFocus"
            },
            {
                "command": "reminder.translate",
                "key": "Alt+X",
                "mac": "Alt+X",
                "when": "editorTextFocus&&editorHasSelection"
            },
            {
                "command": "reminder.removeConsole",
                "key": "Alt+D",
                "mac": "Alt+D"
            },
            {
                "command": "reminder.clearEmptyLines",
                "key": "Alt+V",
                "mac": "Alt+V"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "webpack --mode none",
        "watch": "webpack --mode none --watch",
        "build": "vsce package",
        "build:patch": "vsce package patch"
    },
    "devDependencies": {
        "vsce": "^2.15.0",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "bing-translate-api": "^3.0.0"
    }
}